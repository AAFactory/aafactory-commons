        <activity
            android:name="com.xxmassdeveloper.mpchartexample.CandleStickChartActivity"
            android:theme="@style/AppTheme.NoActionBar"
            android:label="@string/app_name_mp_android_chart">
        </activity>
        <activity
            android:name="com.xxmassdeveloper.mpchartexample.RadarChartActivity"
            android:theme="@style/AppTheme.NoActionBar"
            android:label="@string/app_name_mp_android_chart">
        </activity>
        <activity
            android:name="com.xxmassdeveloper.mpchartexample.FilledLineActivity"
            android:theme="@style/AppTheme.NoActionBar"
            android:label="@string/app_name_mp_android_chart">
        </activity>
        <activity
            android:name="com.xxmassdeveloper.mpchartexample.InvertedLineChartActivity"
            android:theme="@style/AppTheme.NoActionBar"
            android:label="@string/app_name_mp_android_chart">
        </activity>

        <!--https://github.com/bumptech/glide-->
        <activity
            android:name="com.bumptech.glide.samples.imgur.MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity
            android:name="com.bumptech.glide.samples.flickr.FlickrSearchActivity"
            android:launchMode="singleTask"
            android:windowSoftInputMode="stateHidden|adjustResize"
            android:theme="@style/Theme.AppCompat">
        </activity>
        <activity
            android:name="com.bumptech.glide.samples.flickr.FullscreenActivity">
        </activity>

        <!--https://github.com/navermaps/maps.android-->
        <activity
            android:name="com.nhn.android.mapviewer.NMapViewer"
            android:theme="@style/AppTheme"
            android:label="">
        </activity>

        <!--https://github.com/googlesamples/android-ConstraintLayoutExamples-->
        <activity
            android:name="com.example.android.constraintlayoutexamples.MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity android:name="com.example.android.constraintlayoutexamples.ConstraintSetExampleActivity"></activity>
    </application>
</manifest>